{
  "name": "loan-machine-dapp",
  "version": "1.0.0",
  "description": "A decentralized vending machine with borrowing and donating capabilities",
  "keywords": [
    "ethereum",
    "solidity",
    "hardhat",
    "dapp",
    "web3"
  ],
  "license": "MIT",
  "author": "Your Name",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "node": "npx hardhat node",
    "start": "npx http-server src -p 3000 -o",
    "dev": "concurrently \"npx hardhat node\" \"npx http-server src -p 3000 -o\"",
    "clean": "npx hardhat clean"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "dotenv": "^16.6.1",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "hardhat": "^2.17.0",
    "hardhat-deploy": "^0.11.26",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.5"
  }
}
